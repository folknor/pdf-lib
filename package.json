    "Bj Tecu (https://github.com/btecu)",
    "Brent McSharry (https://github.com/mcshaz)",
    "Tim Knapp (https://github.com/duffyd)",
    "Ching Chang (https://github.com/ChingChang9)",
    "François Billioud (https://github.com/Sharcoux)",
    "Phakorn Kiong (https://github.com/PhakornKiong)",
    "Donald Ness (https://github.com/programmarchy)"
  ],
  "scripts": {
    "release": "pnpm release:prep && release-it",
    "release:prep": "pnpm clean && pnpm lint && pnpm typecheck && pnpm test && pnpm build",
    "clean": "rimraf dist coverage",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "testc": "vitest run --coverage",
    "lint": "pnpm exec biome check --write --max-diagnostics=none --reporter=github --formatter-enabled=true --linter-enabled=true --assist-enabled=true",
    "check": "pnpm lint",
    "build": "tsc"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/",
    "src/",
    "LICENSE.md",
    "README.md"
  ],
  "dependencies": {
    "@pdf-lib/standard-fonts": "^1.0.0",
    "@pdf-lib/upng": "^1.0.1",
    "color": "^5.0.3",
    "crypto-js": "^4.2.0",
    "node-html-better-parser": "^1.5.8",
    "pako": "^2.1.0",
    "tslib": "^2.8.1"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.13",
    "@pdf-lib/fontkit": "^1.1.1",
    "@types/color": "^4.2.0",
    "@types/crypto-js": "^4.2.2",
    "@types/pako": "^2.0.4",
    "@vitest/coverage-v8": "^4.0.18",
    "release-it": "^19.2.4",
    "rimraf": "^6.1.2",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  },
  "license": "MIT",
  "private": false,
  "keywords": [
    "pdf-lib",
    "pdf",
    "document",
    "create",
    "modify",
    "creation",
    "modification",
    "edit",
    "editing",
    "typescript",
    "javascript",
    "library"
  ]
}
