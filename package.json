  "scripts": {
    "release": "pnpm release:prep && release-it",
    "release:prep": "pnpm clean && pnpm lint && pnpm typecheck && pnpm test && pnpm build",
    "clean": "rimraf dist coverage",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "testw": "vitest",
    "testc": "vitest run --coverage",
    "lint-staged": "lint-staged",
    "lint": "pnpm lint:prettier && pnpm lint:eslint",
    "lint:eslint": "eslint . --fix",
    "lint:prettier": "prettier --write \"./{src,tests}/**/*.{ts,js,json,html,css}\" --log-level error",
    "build": "tsc"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/",
    "src/",
    "LICENSE.md",
    "README.md"
