{
  "name": "@folknor/pdf-lib",
  "version": "2.6.0",
  "description": "Create and modify PDF files with JavaScript",
  "author": "folknor",
  "packageManager": "pnpm@10.28.2",
  "type": "module",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
